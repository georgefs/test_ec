S'\r\n<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">\r\n<html>\r\n\t<head>\r\n\t\t\t<meta http-equiv="Content-Type" content="text/html; charset=utf-8">\r\n\t\t\t<title>Twodo Girlshop- \xe5\x95\x86\xe5\x93\x81\xe9\xa0\x81</title>\r\n\t\t\t<LINK href="css/main.css" type="text/css" rel="stylesheet">\r\n\t\t\t<script type="text/javascript">\r\n\r\n\tvar _gaq = _gaq || [];\r\n\t_gaq.push([\'_setAccount\', \'UA-38090717-1\']);\r\n\t_gaq.push([\'_trackPageview\']);\r\n\r\n\t(function() {\r\n\t\tvar ga = document.createElement(\'script\'); ga.type = \'text/javascript\'; ga.async = true;\r\n\t\tga.src = (\'https:\' == document.location.protocol ? \'https://ssl\' : \'http://www\') + \'.google-analytics.com/ga.js\';\r\n\t\tvar s = document.getElementsByTagName(\'script\')[0]; s.parentNode.insertBefore(ga, s);\r\n\t})();\r\n\r\n</script>\r\n\r\n<script src="js/jquery.min.js"></script>\r\n<script src="js/stickysidebar.jquery.min.js" type="text/javascript"></script>\r\n<script language="javascript">\r\n\t$(document).ready(function() {\r\n\t\t$("body").css("background-image", "url(/banner/images/20140818102013.jpg)");\r\n\t\t$("#fixed_cart").css("right", ($(window).width() - 1000) / 2 - 80 + "px");\r\n\t\t$(".div_red").each(function() {\r\n\t\t\t$(this).wrap("<div/>");\r\n\t\t\t//$(this).parent().append("<hr/>")\r\n\t\t});\r\n\r\n\t\t$("#main,#header img:not(.item1)").mouseover(function() {\r\n\t\t\t$(".iteml2").hide();\r\n\t\t});\r\n\r\n\t\tif ($(".stickybox").length > 0) {\r\n\t\t\t//$("#main").height($(document).height() - $("#header").height() - $("#footer").height() + 100);\r\n\t\t\t$("#main").css("min-height", $(document).height() - $("#header").height() - $("#footer").height() + "px");\r\n\t\t\t$(".stickybox").stickySidebar({\r\n\t\t\t\tspeed: 500\r\n\t\t\t\t\t\t, padding: 200\r\n\t\t\t\t\t\t, easing: "linear"\r\n\t\t\t\t\t\t, constrain: true\r\n\t\t\t});\r\n\t\t} else {\r\n\t\t\t$("#main").css("min-height", $(document).height() - $("#header").height() - $("#footer").height() + "px");\r\n\t\t}\r\n\t});\r\n\t\r\n\tfunction AddFavorite() {\r\n\t\ttitle = \'Twodo Girlshop\';\r\n\t\turl = \'http://www.twodo.com.tw/\';\r\n\t\tif (window.sidebar) {\r\n\t\t\twindow.sidebar.addPanel(title, url, "");\r\n\t\t} else if (document.all) {\r\n\t\t\twindow.external.AddFavorite(url, title);\r\n\t\t} else if (window.opera && window.print) {\r\n\t\t\talert(\'\xe8\xab\x8b\xe7\x94\xa8 Ctrl + D\xef\xbc\x8c\xe6\x88\x96\xe6\x89\x8b\xe5\x8b\x95\xe5\x8a\xa0\xe5\x85\xa5\xe6\x88\x91\xe7\x9a\x84\xe6\x9c\x80\xe6\x84\x9b\');\r\n\t\t} else if (window.chrome) {\r\n\t\t\talert(\'\xe8\xab\x8b\xe7\x94\xa8 Ctrl + D\xef\xbc\x8c\xe6\x88\x96\xe6\x89\x8b\xe5\x8b\x95\xe5\x8a\xa0\xe5\x85\xa5\xe6\x88\x91\xe7\x9a\x84\xe6\x9c\x80\xe6\x84\x9b\');\r\n\t\t}\r\n\t}\r\n\r\n\tfunction dimImg(objThis, isHover) {\r\n\t\t//alert(isHover);\r\n\t\tif (isHover) {\r\n\t\t\tif ($(objThis).parent().attr("id") != "divDim") {\r\n\t\t\t\t$(objThis).wrap("<div id=\'divDim\' style=\'background-color:black;\'>");\r\n\t\t\t\t$(objThis).fadeTo("fast", 0.8);\r\n\t\t\t}\r\n\t\t} else {\r\n\t\t\t$(objThis).unwrap("<div id=\'divDim\'>");\r\n\t\t\t$(objThis).fadeTo("fast", 1);\r\n\t\t}\r\n\t}\r\n\t\r\n\tfunction swap_img(objThis, isHover, folder) {\r\n//\t\tif (folder) {\r\n//\t\t\t\tif (isHover) {\r\n//\t\t\t\t\t\tif (objThis.src.indexOf("hover") < 0)\r\n//\t\t\t\t\t\t\t\tobjThis.src = objThis.src.replace(folder + \'/\', folder + \'/hover/\');\r\n//\t\t\t\t}else {\r\n//\t\t\t\t\t\tobjThis.src = objThis.src.replace(folder + \'/hover/\', folder + \'/\');\r\n//\t\t\t\t}\r\n//\t\t} else {\r\n\t\t\t\tif (isHover)\r\n\t\t\t\t\t\tobjThis.src = objThis.src.replace(\'.jpg\', \'-h.jpg\');\r\n\t\t\t\telse {\r\n\t\t\t\t\t\tobjThis.src = objThis.src.replace(\'-h.jpg\', \'.jpg\');\r\n\t\t\t\t}\r\n//\t\t}\r\n\t}\r\n\r\n\t\tfunction mySlide(me, duration) {\r\n\t\t\tthis.slideLength = 0;\r\n\t\t\tthis.currentSlide = 0;\r\n\t\t\tthis.obj = me;\r\n\t\t\tthis.duration = duration;\r\n\t\t\tthis.timeout = null;\r\n\t\t}\r\n\r\n\t\t// Set total First\r\n\t\tmySlide.prototype.slide = function() {\r\n\t\t\tvar nodes = this.obj.childNodes;\r\n\t\t\tvar nodesSub;\r\n\t\t\tfor (var i = 0; i < nodes.length; i++) {\r\n\t\t\t\tif (nodes[i].tagName != null && nodes[i] != null && (nodes[i].tagName.toLowerCase() == "img" || nodes[i].tagName.toLowerCase() == "a")) {\r\n\t\t\t\t\tnodesSub = nodes[i].childNodes;\r\n\t\t\t\t\tif (nodesSub.length > 0) {\r\n\t\t\t\t\t\tif (nodesSub[0].tagName && nodesSub[0].tagName.toLowerCase() == "img") {\r\n\t\t\t\t\t\t\tnodesSub[0].tagName && nodesSub[0].setAttribute("style", "display:none;");\r\n\t\t\t\t\t\t\tthis.slideLength++;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tif (nodes[i].tagName && nodes[i].tagName.toLowerCase() == "img") {\r\n\t\t\t\t\t\t\tnodes[i].tagName && nodes[i].setAttribute("style", "display:none;");\r\n\t\t\t\t\t\t\tthis.slideLength++;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\tthis.showSlide(0);\r\n\t\t}\r\n\t\t// Recursive Show images\r\n\t\tmySlide.prototype.showSlide = function(intPos) {\r\n\t\t\tif (this.timeout != null)\r\n\t\t\t\tclearTimeout(this.timeout);\r\n\t\t\tthis.currentSlide = intPos;\r\n\t\t\tif (this.currentSlide == this.slideLength)\r\n\t\t\t\tthis.currentSlide = 0;\r\n\t\t\t$(this.obj).find(".main_rotate_tumb img").css("border", "1px solid #767475");\r\n\t\t\t$(this.obj).find(".main_rotate_tumb img:eq(" + this.currentSlide + ")").css("border", "1px solid #811B30");\r\n\t\t\tvar posCount = 0;\r\n\t\t\tvar nodes = this.obj.childNodes;\r\n\t\t\tvar nodesSub;\r\n\t\t\tfor (var i = 0; i < nodes.length; i++) {\r\n\t\t\t\tnodesSub = nodes[i].childNodes;\r\n\t\t\t\tif (nodesSub.length > 0) {\r\n\t\t\t\t\t//image under anchor <a><img/></a>\r\n\t\t\t\t\tif ((nodesSub[0].tagName && nodesSub[0].tagName.toLowerCase() == "img")\r\n\t\t\t\t\t\t\t\t\t\t\t\t || (nodes[i].tagName && nodes[i].tagName.toLowerCase() == "img")) {\r\n\t\t\t\t\t\t//try IE, if faild do the normal way\r\n\t\t\t\t\t\tobjShowHide(nodesSub[0], false);\r\n\t\t\t\t\t\tif (posCount == this.currentSlide)\r\n\t\t\t\t\t\t\tobjShowHide(nodesSub[0], true);\r\n\t\t\t\t\t\tposCount++;\r\n\t\t\t\t\t}\r\n\t\t\t\t} else {\r\n\t\t\t\t\t//pure image <img/>\r\n\t\t\t\t\tif (nodes[i].tagName && nodes[i].tagName.toLowerCase() == "img") {\r\n\t\t\t\t\t\t//try IE, if faild do the normal way\r\n\t\t\t\t\t\tobjShowHide(nodes[i], false);\r\n\t\t\t\t\t\tif (posCount == this.currentSlide)\r\n\t\t\t\t\t\t\tobjShowHide(nodes[i], true);\r\n\t\t\t\t\t\tposCount++;\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\tvar objSelf = this;\r\n\t\t\tthis.timeout = setTimeout(function() { objSelf.showSlide(objSelf.currentSlide + 1) }, objSelf.duration);\r\n\t\t}\r\n\t\tfunction objShowHide(obj, isShow) {\r\n\t\t\tif (!isShow) {\r\n\t\t\t\t/*\r\n\t\t\t\ttry {\r\n\t\t\t\t\tobj.style.setAttribute(\'cssText\', \'display:none;\', 0);\r\n\t\t\t\t} catch (e) {\r\n\t\t\t\t\tobj.setAttribute("style", "display:none;");\r\n\t\t\t\t}\r\n\t\t\t\t*/\r\n\t\t\t\t$(obj).fadeOut("fast");\r\n\t\t\t} else {\r\n\t\t\t\t/*\r\n\t\t\t\ttry {\r\n\t\t\t\t\tobj.style.setAttribute(\'cssText\', \'display:block;\', 0);\r\n\t\t\t\t} catch (e) {\r\n\t\t\t\t\tobj.setAttribute("style", "display:block;");\r\n\t\t\t\t}\r\n\t\t\t\t*/\r\n\t\t\t\t$(obj).fadeIn("fast");\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t\tfunction setBlock(intBlock, intTotalBlock) {\r\n\t\t\tfor (var i = 1; i < intTotalBlock + 1; i++) {\r\n\t\t\t\tobjShowHide(document.getElementById("div_mri" + i), false);\r\n\t\t\t\tif (i == intBlock)\r\n\t\t\t\t\tobjShowHide(document.getElementById("div_mri" + i), true);\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t\t//********** Mask ****************\r\n\t\tfunction myMask(me, duration, imgOn) {\r\n\t\t\tthis.obj = me;\r\n\t\t\tthis.duration = duration;\r\n\t\t\tthis.imgOff = this.obj.attr("src");\r\n\t\t\tthis.imgOn = imgOn;\r\n\t\t\tthis.isFinished = false;\r\n\r\n\t\t\tvar x = this;\r\n\t\t\t$(this.obj).closest("div").mouseover(function() {\r\n\t\t\t\tx.on();\r\n\t\t\t});\r\n\t\t\t$(this.obj).closest("div").mouseout(function() {\r\n\t\t\t\tx.off();\r\n\t\t\t});\r\n\t\t}\r\n\t\tmyMask.prototype.on = function() {\r\n\t\t\tif (this.obj.attr("src") == this.imgOff) {\r\n\r\n\t\t\t\tvar x = this;\r\n\t\t\t\tx.isFinished = false;\r\n\r\n\t\t\t\tthis.obj.attr("src", this.imgOn);\r\n\t\t\t\tvar h = $(this.obj).height();\r\n\t\t\t\t$(this.obj).css("position", "absolute");\r\n\t\t\t\t$(this.obj).css({ "top": "-" + (h - 20) + "px","left":"0px" });\r\n\r\n\t\t\t\t$(this.obj).stop(true, true).animate({ "top": "0px", "left": "0px", "queue": false }, this.duration, \'linear\', function() {\r\n\t\t\t\t\t$(this).css("position", "relative");\r\n\t\t\t\t\t$(this).css("top", "0px").css("width", "216px").css("height", "326px");\r\n\t\t\t\t\tx.isFinished = true;\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t}\r\n\t\tmyMask.prototype.off = function() {\r\n\t\t\tif (this.obj.attr("src") == this.imgOn && this.isFinished) {\r\n\t\t\t\t$(this.obj).stop(true, true);\r\n\t\t\t\tthis.obj.attr("src", this.imgOff);\r\n\t\t\t\t$(this.obj).css("position", "relative");\r\n\t\t\t\t$(this.obj).css({ "top": "0px", "left": "0px", "width": "216px", "height": "326px" });\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t\t/********** photoSlide_thumbbar ************/\r\n\t\tfunction photoSlide_thumbbar(me, bgcolor, color, bgcolor_hover, duration, fadeout, fadein) {\r\n\t\t\tthis.slideLength = 0;\r\n\t\t\tthis.currentSlide = 0;\r\n\t\t\tthis.obj = me;\r\n\t\t\tthis.bgcolor = bgcolor;\r\n\t\t\tthis.color = color;\r\n\t\t\tthis.bgcolor_hover = bgcolor_hover;\r\n\t\t\tthis.duration = duration;\r\n\t\t\tthis.durationFO = fadeout;\r\n\t\t\tthis.durationFI = fadein;\r\n\t\t\tthis.timeout = null;\r\n\r\n\t\t\tvar d = $("<div style=\'width:960px\'></div>");\r\n\t\t\t$(d).width($(this.obj).find("img").length * 960);\r\n\t\t\t$(d).append($(this.obj).html());\r\n\t\t\t$(this.obj).html("");\r\n\t\t\t$(this.obj).append(d);\r\n\r\n\t\t\t$(me).find("img").addClass("slide