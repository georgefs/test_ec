\r\n\t\t\tthis.currentSlide++;\r\n\t\t\tif (this.currentSlide > this.slideLength) {\r\n\t\t\t\tclearTimeout(this.timeout);\r\n\t\t\t\tthis.currentSlide = 0;\r\n\t\t\t}\r\n\t\t\tthis.show(this.currentSlide);\r\n\t\t\tvar x = this;\r\n\t\t\tthis.timeout = setTimeout(function() { x.auto() }, this.duration);\r\n\t\t}\r\n\t\tphotoSlide_thumbbar.prototype.stop = function() {\r\n\t\t\tclearTimeout(this.timeout);\r\n\t\t\t//var x = this;\r\n\t\t\t//this.timeout = setTimeout(function() { x.auto() }, this.duration);\r\n\t\t}\r\n\t\tphotoSlide_thumbbar.prototype.show = function(index) {\r\n\t\t\tif (index < 0) {\r\n\t\t\t\t//alert("Min!");\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t\tif (index > this.slideLength - 1) {\r\n\t\t\t\t//alert("Max!");\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t\tthis.currentSlide = index;\r\n\r\n\t\t\tthis.scrollThumb(index);\r\n\r\n\t\t}\r\n\t\tphotoSlide_thumbbar.prototype.scrollThumb = function(index) {\r\n\t\t\t//alert(index);\r\n\t\t\tif (index == 0) {\r\n\t\t\t\t$(this.obj).animate({ scrollLeft: index * 960 }, 0);\r\n\t\t\t\t$("#brand_main_thumb").scrollLeft(0);\r\n\t\t\t} else {\r\n\t\t\t\t$(this.obj).animate({ scrollLeft: index * 960 }, 500);\r\n\t\t\t\t$("#brand_main_thumb").scrollLeft((1 + (index - 3) * (159 + 1)));\r\n\t\t\t}\r\n\t\t}\r\n\t</script>\r\n\t</head>\r\n\t<body>\r\n\t\t<form name="formlogout" method="post" action="Login.asp">\r\n<input type="hidden" name="btnlogin" value="logout">\r\n</form>\r\r\n<div id="header">\r\n\t\r\n\t<table>\r\n\t\t<tr>\r\n\t\t\t<td>\r\n\t\t\t\t<a href="Default.asp"><img src="images/index/index_01.jpg" /></a>\r\n\t\t\t</td>\r\n\t\t\t<td width="193"></td>\r\n\t\t\t<td>\r\n\t\t\t\t<table>\r\n\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t<td>\r\n\t\t\t\t\t\t\t<div style="border-bottom:5px solid black;">\r\n\t\t\t\t\t\t\t\t<table cellpadding="10" align="right">\r\n\t\t\t\t\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t\t\t\t\t<td align="right">\r\n\t\t\t\t\t\t\t\t\t\t\t\r\n\t\t\t\t\t\t\t\t\t\t\t\t<a href="Login.asp">\xe7\x99\xbb\xe5\x85\xa5</a>\r\n\