();\r\n                var i, j = d.MM_p.length, a = MM_preloadImages.arguments; for (i = 0; i < a.length; i++)\r\n                    if (a[i].indexOf("#") != 0) { d.MM_p[j] = new Image; d.MM_p[j++].src = a[i]; } \r\n            }\r\n        }\r\n\r\n        function MM_findObj(n, d) { //v4.01\r\n            var p, i, x; if (!d) d = document; if ((p = n.indexOf("?")) > 0 && parent.frames.length) {\r\n                d = parent.frames[n.substring(p + 1)].document; n = n.substring(0, p);\r\n            }\r\n            if (!(x = d[n]) && d.all) x = d.all[n]; for (i = 0; !x && i < d.forms.length; i++) x = d.forms[i][n];\r\n            for (i = 0; !x && d.layers && i < d.layers.length; i++) x = MM_findObj(n, d.layers[i].document);\r\n            if (!x && d.getElementById) x = d.getElementById(n); return x;\r\n        }\r\n\r\n        function MM_swapImage() { //v3.0\r\n            var i, j = 0, x, a = MM_swapImage.arguments; document.MM_sr = new Array; for (i = 0; i < (a.length - 2); i += 3)\r\n                if ((x = MM_findObj(a[i])) != null) { document.MM_sr[j++] = x; if (!x.oSrc) x.oSrc = x.src; x.src = a[i + 2]; }\r\n        }\r\n\r\n    </script>\r\n    \r\n<style type"text/css">\r\n    \r\nbody{margin:0px;padding:0px;font-family:Arial;}\r\na img,:link img,:visited img { border: none; }\r\ntable { border-collapse: collapse; border-spacing: 0; }\r\n:focus { outline: none; }\r\n*{margin:0;padding:0;}\r\np, blockquote, dd, dt{margin:0 0 8px 0;line-height:1.5em;}\r\nfieldset {padding:0px;padding-left:7px;padding-right:7px;padding-bottom:7px;}\r\nfieldset legend{margin-left:15px;padding-left:3px;padding-right:3px;color:#333;}\r\ndl dd{margin:0px;}\r\ndl dt{}\r\n\r\n.clearfix:after{clear:both;content:".";display:block;font-size:0;height:0;line-height:0;visibility:hidden;}\r\n.clearfix{display:block;zoom:1;}\r\n\r\n\r\nul#thumblist{display:block;}\r\nul#thumblist li{float:left;margin-right:2px;list-style:none; height: 64px;}\r\nul#thumblist .li-2{float:left;margin-right:2px;list-style:none; height: 40px;}\r\nul#thumblist .li-2 a{display:block;border:1px solid #CCC; margin-left:2px; margin-right:2px;}\r\nul#thumblist li a{display:block;border:1px solid #CCC; margin-left:5px; margin-right:5px;}\r\nul#thumblist li a.zoomThumbActive{\r\n    border:2px solid #01c3bc; margin-left:5px; margin-right:5px;\r\n}\r\n\r\n.jqzoom{\r\n\r\n\ttext-decoration:none;\r\n\tfloat:left;\r\n}\r\n\r\n</style>\r\n\r\n<script type="text/javascript">\r\n    $(document).ready(function () {\r\n\r\n        var orgcolor;\r\n        $(".L-side li a span").mouseover(function () {\r\n            orgcolor = $(this).css("color");\r\n            $(this).css("color", "#34acab");\r\n        });\r\n        $(".L-side li a span").mouseout(function () {\r\n            $(this).css("color", orgcolor);\r\n        });\r\n\r\n        if ($("#errtxt").val() != "") {\r\n            alert($("#errtxt").val());\r\n            $("#errtxt").val("");\r\n        }\r\n\r\n        $(\'.carousel\').each(function (index) {\r\n            $(this).find(\'.jCarouselLite\').each(function (index) {\r\n                $(this).jCarouselLite({\r\n                    btnPrev: $(this).parent().find(\'.prev\'),\r\n                    btnNext: $(this).parent().find(\'.next\'),\r\n                    visible: 3,\r\n                    vertical: true,\r\n                    circular: false\r\n                });\r\n            });\r\n        });\r\n\r\n        $(\'.commall\').each(function (index) {\r\n            $(this).find(\'.comm_imgR\').each(function (index) {\r\n                $(this).jCarouselLite({\r\n                    btnPrev: $(this).parent().find(\'.prev\'),\r\n                    btnNext: $(this).parent().find(\'.next\'),\r\n                    visible: 2,\r\n                    vertical: false,\r\n                    circular: false\r\n                });\r\n            });\r\n        });\r\n        \r\n        $(\'.jqzoom\').jqzoom({\r\n            zoomType: \'reverse\',\r\n            lens: true,\r\n            preloadImages: false,\r\n            alwaysOn: false\r\n        });\r\n\r\n        if ($("#hiddenNoStock").val() == "Y") {\r\n            $("#ddlCount").append($(\'<option>\', {\r\n                value: 0,\r\n                text: "\xe7\x84\xa1\xe5\xba\xab\xe5\xad\x98"\r\n            }));\r\n        }\r\n\r\n        //\xe5\x8a\xa0\xe5\x85\xa5\xe8\xb3\xbc\xe7\x89\xa9\xe8\xbb\x8a\r\n        $("#LinkButtonAddCart1").click(function () {\r\n            if ($("#hiddenColor").val() == "") {\r\n                alert("\xe9\xa1\x8f\xe8\x89\xb2\xe6\x9c\xaa\xe9\x81\xb8\xe6\x93\x87");\r\n                return false;\r\n            }\r\n            if ($("#hiddenSize").val() == "") {\r\n                alert("\xe5\xb0\xba\xe5\xaf\xb8\xe6\x9c\xaa\xe9\x81\xb8\xe6\x93\x87");\r\n                return false;\r\n            }\r\n            if ($("#hiddenSelectProductID").val() == "") {\r\n                alert("\xe8\xa6\x8f\xe6\xa0\xbc\xe6\x9c\xaa\xe9\x81\xb8\xe6\x93\x87");\r\n                return false;\r\n            }\r\n            if ($("#hiddenSelectCount").val() == "0") {\r\n                alert("\xe7\x94\xa2\xe5\x93\x81\xe7\x84\xa1\xe8\xb2\xa8");\r\n                return false;\r\n            }\r\n            try {\r\n                sendBeacon(); //\xe9\x80\x99\xe5\x80\x8bfunction\xe5\xae\x89\xe8\xa3\x9d\xe5\x9c\xa8GTM\xe4\xb8\xadvisit.js\r\n            }\r\n            catch (e) { }\r\n            \r\n            $.ajax({\r\n                type: "POST",\r\n                url: "product.aspx",\r\n                async: false,\r\n                data: { Func: "AddCart", ProductID: $("#hiddenSelectProductID").val(), SelectCount: $("#hiddenSelectCount").val(), hiddenURL: $("#hiddenURL").val() },\r\n                success: function (data) {\r\n                    if (data.substring(0, 3) == "OK:") {\r\n                        var AddName = $("#Label\xe7\xb3\xbb\xe5\x88\x97\xe5\x90\x8d\xe7\xa8\xb11").html();\r\n                        if (AddName.length > 15) {\r\n                            AddName = AddName.substring(0, 15) + ".....";\r\n                        }\r\n\r\n                        var Amount = parseInt($("#hiddenSelectCount").val()) * parseInt($("#hiddenPrice").val());\r\n                        var TotalAmount = Amount;\r\n                        var TotalCount = parseInt($("#hiddenCartCount").val()) + parseInt($("#hiddenSelectCount").val());\r\n\r\n                        if ($("#liShowCart_Total").length > 0) {\r\n                            TotalAmount = parseInt($("#liShowCart_Total").html().split("$")[1]) + Amount;\r\n                        }\r\n                        else {\r\n                            var html = [];\r\n                            html.push(\'<ul style="background-color:#FFF;">\');\r\n                            html.push(\'<li class="li-01" id="liShowCart_Name"><span style="color:#2c7987">\xe5\x93\x81\xe5\x90\x8d</span></li>\');\r\n                            html.push(\'<li class="li-02" id="liShowCart_Spec"><span style="color:#2c7987">\xe8\xa6\x8f\xe6\xa0\xbc</span></li>\');\r\n                            html.push(\'<li class="li-02" id="liShowCart_Count"><span style="color:#2c7987">\xe6\x95\xb8\xe9\x87\x8f</span></li>\');\r\n                            html.push(\'<li class="li-02" id="liShowCart_Amount"><span style="color:#2c7987">\xe5\xb0\x8f\xe8\xa8\x88</span></li>\');\r\n                            html.push(\'<li class="li-03">\');\r\n                            html.push(\'<br><span style="color:#F39" id="liShowCart_Total"></span>\');\r\n                            html.push(\'<br><a href="./cart01.aspx"><img src="http://image.obdesign.com.tw/images/ObdesignBanner/13-0318index/adImg/order-ico.gif" border="0"></a>\');\r\n                            html.push(\'</li>\');\r\n                            html.push(\'</ul>\');\r\n\r\n                            $("#ShowCartLi-5").append(html.join(\'\'));\r\n                        }\r\n\r\n                        $("#liShowCart_Name").append("<br>" + AddName);\r\n                        $("#liShowCart_Spec").append("<br>" + $("#hiddenColor").val() + "," + $("#hiddenSize").val());\r\n                        $("#liShowCart_Count").append("<br>" + $("#hiddenSelectCount").val());\r\n                        $("#liShowCart_Amount").append("<br>" + Amount);\r\n                        $("#liShowCart_Total").html("\xe7\xb8\xbd\xe9\x87\x91\xe9\xa1\x8d\xef\xbc\x9a$" + TotalAmount);\r\n                        $("#ShowCart_TotalCount").html("(" + TotalCount + "\xe4\xbb\xb6)");\r\n                        $("#hiddenCartCount").val(TotalCount);\r\n\r\n                        var YWAData = data.substring(3).split("|");\r\n                        doAddToCart(YWAData[0], YWAData[1], YWAData[2], YWAData[3], YWAData[4]);\r\n                    }\r\n                },\r\n                error: function () {\r\n                }\r\n            });\r\n\r\n            return false;\r\n        });\r\n\r\n        //\xe5\x8a\xa0\xe5\x85\xa5\xe8\xb2\xa8\xe5\x88\xb0\xe9\x80\x9a\xe7\x9f\xa5\xe6\xb8\x85\xe5\x96\xae\r\n        $("#LinkButtonAddWishList").click(function () {\r\n            $.ajax({\r\n                type: "POST",\r\n                url: "product.aspx",\r\n                async: false,\r\n                data: { Func: "AddWishList", ProductID: $("#hiddenSelectProductID").val() },\r\n                success: function (data) {\r\n                    if (data.substring(0, 3) == "OK:") {\r\n                        alert(\'\xe5\xb7\xb2\xe5\x8a\xa0\xe5\x85\xa5"\xe8\xb2\xa8\xe5\x88\xb0\xe9\x80\x9a\xe7\x9f\xa5"\xe6\xb8\x85\xe5\x96\xae\');\r\n                    } else {\r\n                        alert(data.substring(4));\r\n                    }\r\n                },\r\n                error: function () {\r\n                }\r\n            });\r\n\r\n            return false;\r\n        });\r\n\r\n        $(".btnColor").click(function () {\r\n            var Color = $(this).html(); //\xe7\x9b\xae\xe5\x89\x8d\xe9\xbb\x9e\xe9\x81\xb8\xe7\x9a\x84\xe9\xa1\x8f\xe8\x89\xb2\r\n            if ($(this).attr("checkEnabled") == "False" || Color == $("#hiddenColor").val()) { return false; } //checkEnabled=False\xe6\x88\x96\xe9\xbb\x9e\xe9\x81\xb8\xe5\xb7\xb2\xe9\xbb\x9e\xe9\x81\xb8\xe7\x9a\x84\xe9\xa1\x8f\xe8\x89\xb2\xe5\x89\x87\xe4\xb8\x8d\xe5\x81\x9a\xe4\xba\x8b\r\n            if ($(this).attr("enabled") == "False" && $("#hiddenColor").val() == "") { return false }\r\n            $(".choose_style").removeClass("choose_style").addClass("color_style"); //\xe5\xb0\x87\xe5\xb7\xb2\xe9\xbb\x9e\xe9\x81\xb8\xe7\x9a\x84\xe9\xa1\x8f\xe8\x89\xb2\xe5\x9b\x9e\xe5\xbe\xa9\xe7\x82\xba\xe9\xa0\x90\xe8\xa8\xadclass\r\n            $(this).removeClass("no_colorHaveCursor").removeClass("color_style").addClass("choose_style"); //\xe5\xb0\x87\xe7\x9b\xae\xe5\x89\x8d\xe9\xbb\x9e\xe9\x81\xb8\xe7\x9a\x84\xe9\xa1\x8f\xe8\x89\xb2\xe6\x8f\x9b\xe6\x88\x90\xe9\xbb\x9e\xe9\x81\xb8\xe4\xb8\xad\xe7\x9a\x84class\r\n            $("#hiddenColor").val(Color); //\xe8\xa8\x98\xe9\x8c\x84\xe7\x9b\xae\xe5\x89\x8d\xe9\xbb\x9e\xe9\x81\xb8\xe9\xa1\x8f\xe8\x89\xb2\r\n            $("#ddlCount").html("");\r\n            $("#hiddenSelectCount").val("");\r\n            $("#hiddenSelectProductID").val("");\r\n            $("#hiddenPrice").val("0");\r\n\r\n            $("#LinkButtonAddCart1").show();\r\n            $("#LinkButtonAddWishList").hide();\r\n\r\n            $(".btnSize[checkEnabled=\'True\']").each(function () {\r\n                var Stock = parseInt($(".ALLData[Color=\'" + Color + "\'][Size=\'" + $(this).html() + "\']").attr("Stock"));\r\n                if (Stock > 0) { //\xe6\x9c\x89\xe5\xba\xab\xe5\xad\x98\r\n                    $(this).removeClass("size").removeClass("no_size").addClass("size").attr("enabled", "True");\r\n                }\r\n                else { //\xe7\x84\xa1\xe5\xba\xab\xe5\xad\x98\r\n                    $(this).removeClass("size").removeClass("no_size").addClass("no_si